    {template .index}
        {call aui.page.document}
            {param windowTitle: 'Window title text' /}
            {param headContent}
                <!-- HEAD content such as CSS and JS resources -->
            {/param}
            {param content}
                {call aui.page.page}
                    {param headerContent}
                        <!-- call aui.page.header here -->
                    {/param}
                    {param contentContent}
                        <!-- call aui.page.pagePanel here -->
                    {/param}
                    {param footerContent}
                        <!-- literal content here -->
                    {/param}
                {/call}
            {/param}
        {/call}
    {/template}
